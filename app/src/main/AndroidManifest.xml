<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dawar.jewellerybilling">

    <application
        android:allowBackup="true"
        android:icon="@drawable/jwello_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/jwello_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".customers.AddCustomerActivity"></activity>
        <activity android:name=".customers.CustomersActivity" />
        <activity
            android:name=".settings.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".billing.BillingActivity" />
        <activity android:name=".SetUpActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>